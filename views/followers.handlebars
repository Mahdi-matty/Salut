
<div class="row">
  <div class="col">
{{!-- <p>Do I work?</p> --}}
        <h3>Followers</h3>
    {{!-- <div class="collapse multi-collapse" id="multiCollapseExample1"> --}}
    <div>
      <div class="card card-body" id="followers-container">
        {{#each followers as |follower|}}
        <ul class="list-group">
            <li class="list-group-item">
                {{follower.username}}
                {{#if follower.Follow.id}}
                <span class="badge bg-primary">Following</span>
              {{/if}}
            </li>
        </ul>
        {{/each}}
      </div>
    </div>
  </div>
  <div class="col">
    {{!-- <div class="collapse multi-collapse" id="multiCollapseExample2"> --}}
    <div>

        <h3>Following</h3>
      <div class="card card-body" id="following-container">
         {{#each followings as |following|}}
        <ul class="list-group">
            <li class="list-group-item">
                {{following.username}}
                {{#if following.Follow.id}}
                <span class="badge bg-primary">Following</span>
              {{/if}}
            </li>
        </ul>
        {{/each}}
      </div>
    </div>
  </div>
</div>

<script src="/js/follow.js"></script>